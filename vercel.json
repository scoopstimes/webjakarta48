{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/home.html"
    },
    {
      "source": "/members",
      "destination": "/components/page/members"
    },
    {
      "source": "/news",
      "destination": "/components/page/news"
    },
    {
      "source": "/schedule",
      "destination": "/components/page/schedule"
    },
    {
      "source": "/mypage",
      "destination": "/components/login/mypage"
    },
    {
      "source": "/login",
      "destination": "/components/login/login"
    },
    {
      "source": "/about",
      "destination": "/components/page/about"
    },
    {
      "source": "/terms",
      "destination": "/components/page/terms"
    },
    {
      "source": "/support",
      "destination": "/components/page/support"
    },
    {
      "source": "/ssk",
      "destination": "/components/page/ssk"
    },
    {
      "source": "/maintenance",
      "destination": "/middleware/maintenance"
    },
    {
      "source": "/member/:profileId",
      "destination": "/components/detail/member?id=:profileId"
    }, 
    {
      "source": "/theater/:setlist",
      "destination": "/components/detail/theater?setlist=:setlist"
    },
    {
      "source": "/schedule/month/:month",
      "destination": "/components/page/schedule/month/:month"
    },
    {
      "source": "/member/:id",
      "destination": "/components/detail/member?id=:id"
    },
    {
      "source": "/news/:id",
      "destination": "/components/detail/news?id=:id"
    },
    {
      "source": "/:path*/:slug",
      "destination": "/:path*/:slug.html"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/components/detail/:slug",
      "destination": "/components/detail/:slug.html"
    },
    {
      "source": "/middleware/:slug",
      "destination": "/middleware/:slug.html"
    },
    {
      "source": "/components/page/:slug",
      "destination": "/components/page/:slug.html"
    }
  ]
}
